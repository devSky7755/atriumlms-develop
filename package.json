  "scripts": {
    "graphql:generate": "graphql-codegen --config codegen.yml",
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "analyze": "cross-env ANALYZE=true next build",
    "start": "cross-env NODE_ENV=production next start",
    "eslint:check": "eslint --max-warnings 0 './**/*.+(+(c|m)js|+(j|t)s?(x)|mdx)'",
    "eslint:fix": "yarn eslint:check --fix",
    "prettier:check": "prettier --config ./.prettierrc.json --check --ignore-unknown '*'",
    "prettier:fix": "yarn prettier:check --write",
    "stylelint:check": "stylelint '**/*.+(css|scss)'",
    "stylelint:fix": "yarn stylelint:check --fix",
    "lint:check": "concurrently --prefix '[{name}]' 'npm:eslint:check' 'npm:stylelint:check'",
    "lint:fix": "concurrently --prefix '[{name}]' 'npm:eslint:fix' 'npm:stylelint:fix'",
    "prepare": "husky install",
    "ncu:latest": "ncu -u --target latest",
    "ncu:minor": "ncu -u --target minor",
    "ncu:patch": "ncu -u --target patch"
